    "@types/jest": "^27.0.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^20.2.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.41.0",
    "jest": "^27.2.0",
